{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/profile",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/trip/edit/*",
      "allowedRoles": [
        "contributor"
      ]
    },
    {
      "route": "/trip/new/*",
      "allowedRoles": [
        "contributor"
      ]
    },
    {
      "route": "/api/*",
      "methods": [
        "PUT",
        "POST",
        "PATCH",
        "DELETE"
      ],
      "allowedRoles": [
        "contributor"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    }
  }
}